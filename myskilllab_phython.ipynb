        "def greet(name):\n",
        "    if name:\n",
        "        print(f\"Hello, {name}!\")\n",
        "    else:\n",
        "        print(\"Hello, Guest!\")\n",
        "\n",
        "def main():\n",
        "    user_name = input(\"Enter your name: \")\n",
        "    greet(user_name)\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ]
